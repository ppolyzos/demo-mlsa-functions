{
    "version": "2.0",
    "functions": [
        "Chaining_Hello",
        "Chaining_SayHello",
        "Chaining_SayHello_Direct",
        "Chaining_Hello_HttpStart",
        "Monitor_Hello",
        "Monitor_SayHello",
        "Monitor_SayHello_Direct",
        "Monitor_Hello_HttpStart_CustomStatus",
        "Monitor_Hello_HttpStart_RuntimeStatus"
    ],
    "functionTimeout": "00:05:00",
    "healthMonitor": {
        "enabled": true,
        "healthCheckInterval": "00:00:10",
        "healthCheckWindow": "00:02:00",
        "healthCheckThreshold": 6,
        "counterThreshold": 0.8
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true
            }
        }
    }
}